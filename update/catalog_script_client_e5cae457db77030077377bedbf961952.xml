        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }
	var choicetest1 = g_form.getValue('test1');
	var risk= parseInt(g_form.getValue('risk'));
	
	if(choicetest1 == 'true'){
		if(risk){
			risk += 100;
		}else{
		    risk = 100;
		}

	}else if(choicetest1 == 'false'){
		if(risk){
			risk = risk - 100;
		}else{
		    risk = '';
		}
	}
	
	if(risk){
		
		g_form.setValue('risk', risk);
	}else{
		g_form.clearValue('risk');
	}
}]]></script>
